{
  "name": "fast-food-fast",
  "version": "0.0.0",
  "private": true,
  "main": "app.js",
  "scripts": {
    "build": "babel -d ./build ./src",
    "start": "npm run build && node ./build/app.js",
    "dev": "babel-node ./src/app.js",
    "test": "mocha --recursive './test/index.js' --compilers js:babel-core/register --exit",
    "cover": "nyc npm run test && nyc report --reporter=lcov --reporter=text-lcov npm test",
    "coveralls": "nyc report --reporter=lcov --reporter=text-lcov | ./node_modules/coveralls/bin/coveralls.js"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.16.1",
    "cors": "^2.8.4",
    "debug": "^3.0.0",
    "del": "^3.0.0",
    "dotenv": "^6.0.0",
    "express": "^4.15.4",
    "jsonwebtoken": "^8.3.0",
    "morgan": "^1.8.2",
    "nodemailer": "^4.6.8",
    "nyc": "^12.0.2",
    "pg": "^7.4.3",
    "sequelize": "^4.38.0",
    "swagger-ui-express": "^4.0.0",
    "url": "^0.11.0",
    "uuid": "^3.3.2",
    "validator": "^10.7.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "chai": "^4.1.1",
    "chai-http": "^4.0.0",
    "coveralls": "^3.0.2",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-mocha": "^4.11.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0"
  }
}
